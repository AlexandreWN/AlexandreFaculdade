public class Missao1_principal {

	public static void main(String[] args) {
		System.out.println("Inicio da Missão 1");
		//Declaração de variaveis
		int Num1 = 0, Num2 = 0, Resultado = 0;
		
		//Atribuir um valor para as variaveis
		Num1 = 5;
		Num2 = 6;
		
		//Realiza a soma dos valores e insere na variavel Resultado
		Resultado = Num1 + Num2;
		
		//Printa o resultado para o usuario
		System.out.printf("O resultado de sua soma foi:%d",Resultado);
	}

}
